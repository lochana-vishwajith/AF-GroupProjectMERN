nav {
  background-color: black;
}
h3 {
  margin-top: 20px;
}
hr {
  margin-bottom: 30px;
}
.comitteeDiv {
  display: grid;
  grid-template-columns: 2fr 2fr 2fr;
  box-sizing: border-box;
  margin-top: 20px;
}
.image {
  width: 260px;
  border-radius: 27em;
  max-width: 260px;
}

.btnDiv {
  display: grid;
  grid-template-columns: 1fr 1fr;
  box-sizing: border-box;
}
